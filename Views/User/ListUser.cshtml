@{
    ViewData["Title"] = "List User";
}
<h1>@ViewData["Title"]</h1>

<table class="table table-default">
    <thead>
        <tr>
            <th>ID</th>
            <th>User</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Microsoft.AspNetCore.Identity.IdentityUser u in Model)
        {
            <tr>
                <td>@u.Id</td>
                <td>@u.UserName</td>
                <td>
                    
                </td>
            </tr>
        }
    </tbody>
</table>